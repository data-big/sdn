CREATE EXTERNAL TABLE vpnpostion(id string,realNumber String,bizIP String,sac string,lac string,address String,time string) STORED BY 'org.apache.hadoop.hive.hbase.HBaseStorageHandler' WITH SERDEPROPERTIES ("hbase.columns.mapping" = ":key,vpnpostion:realNumber,vpnpostion:bizIP,vpnpostion:sac,vpnpostion:lac,vpnpostion:address,vpnpostion:time") TBLPROPERTIES("hbase.table.name" = "sdn");
